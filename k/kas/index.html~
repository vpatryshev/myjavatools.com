  <script type="text/javascript">
    document.write("your browser is: " + navigator.userAgent);

    function runTest(name) {
      var splitPath = document.location.toString().split("/");
      if (splitPath[0] == "http:") {
        alert("Sorry, JSUnit cannot run tests from the server. Please open this page as a file, if you can.");
      } 
      
      var jsunitPath = splitPath.slice(0, -2).join("/") +
                     "/third_party/java/jsunit/jsunit2.2/testRunner.html";
      var testPath = splitPath.slice(0, -1).join("/") + "/" + name;
      var url = jsunitPath + "?testPage=" + testPath + "&autoRun=true";
      window.open(url, "keyboard_unittest");
      return true;
    }
  </script>

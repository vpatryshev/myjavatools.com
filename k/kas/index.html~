        for (var locale in data) {
          var entry = data[locale];
          if (entry.bin) {
            var dName = 'd_' + locale + '_002.js';
            var fName = 'f_' + locale + '_002.js';
            var gadgetName = 'kbd_' + locale + '_gadget.xml';
            var greasemonkeyName = 'keyboard_' + locale + '.user.js';
            var binPage = entry.page ? ('Google_' + locale + '.html') :
                          ('Google.html?nl=' + locale);
            var srcPage = entry.page ? ('Google_' + locale + '_src.html') :
                          ('Google_src.html?nl=' + locale);
            _('<tr><td>', entry.title, '<br></td>');
            _('<td align="center"><a href="', binPage, '">');
            _("try", '</a> ');
            _('<a style="font-size:60%" href="#"');
            _('onclick=\'runTest("keyboard_', locale, '_bin_unittest.html")\'>unittest</a>');
            _('</td>');
//            _('<td align="center"><a href="', srcPage, '">try</a> ');
//            _('<a style="font-size:60%" href="#"');
//            _('onclick=\'runTest("javascript/keyboard/keyboard_', locale);
//            _('_unittest.html")\'>unittest</a>');
//            _('</td>');
            _('<td align="center"><a href="bin/', dName, '">click</a>');
//            _('/<a href="bin/', fName, '">fixed</a>');
            if (gadgetDo) {
              _('<td align="center"><a href="gadgets/', gadgetName, '">url</a></td>');
            }
            if (monkeyDo) {
              _('<td align="center"><a href="greasemonkey/', greasemonkeyName, '">install</a></td>');
            }
            _('<td align="center">', bookmarklet(locale, dName, entry.title));
            _('</td></tr>');
          }
        }

        function bookmarklet(langId, scriptName, title) {
          return ['<a href=\'javascript:(function(){var%20d=document;var%20s=d.createElement("script");s.onreadystatechange=s.onload=function(){window.onload()};s.src="bin/',
              scriptName,
              '";d.body.appendChild(s);})();\' style="border: 1px solid rgb(170, 170, 170); padding: 0.1em 0.5em; background-color: rgb(153, 153, 153); color: rgb(255, 255, 255); text-decoration: none; font-weight: bold; text-shadow: 2px 2px 2px rgb(102, 102, 102);" title="',
            title,
            ': drag this to your bookmarks bar">',
            langId,
            '</a>'].join('');
        }
